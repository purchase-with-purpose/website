{
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      },
      "label": {
        "type": "string"
      },
      "logo": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "indicators": {
        "type": "array",
        "items": {
          "type": "string",
          "enum": [
            "environmental",
            "open-source",
            "privacy",
            "self-hosted"
          ]
        }
      },
      "incumbent": {
        "type": "boolean"
      },
      "origin": {
        "type": "string",
        "enum": [
          "us",
          "eu"
        ]
      },
      "category": {
        "type": "string",
        "enum": [
          "photo-management",
          "browser",
          "file-storage",
          "office-suite",
          "search-engine",
          "audio-book",
          "none"
        ]
      },
      "tags": {
        "type": "array",
        "items": {
          "type": "string",
          "enum": [
            "global.test",
            "photo-management.a",
            "photo-management.b"
          ]
        }
      },
      "notes": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "variant": {
              "type": "string",
              "enum": [
                "info",
                "disclaimer",
                "warning"
              ]
            },
            "content": {
              "type": "string"
            }
          },
          "required": [
            "variant",
            "content"
          ],
          "additionalProperties": false
        }
      }
    },
    "required": [
      "id",
      "label",
      "logo",
      "description",
      "indicators",
      "incumbent",
      "origin",
      "category",
      "tags",
      "notes"
    ],
    "additionalProperties": false
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}